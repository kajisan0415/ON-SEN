<div class="article-show">
<h2>あああ</h2>
<%= @user.name %>
<p>Lv.<%= @user.stamped_articles.count %></p>
<% if @user.stamped_articles.count < 10 %>
<P>温泉ビギナー</P>
<% end %>

<% @user.stamped_articles.each do |article| %>
<%= link_to article_path(article) do %>
<%= attachment_image_tag(article, :image, :fill, 100, 100) %>
<% end %>
<% end %>

<% if @user.id == current_user.id %>
<%= link_to '行きたいリスト', favorites_users_path %>
<% end %>

</div>